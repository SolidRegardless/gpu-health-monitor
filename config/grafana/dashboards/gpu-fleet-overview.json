          "format": "table",
          "rawSql": "SELECT \n  m.gpu_uuid,\n  a.datacenter,\n  ROUND(m.gpu_temp::numeric, 1) as gpu_temp,\n  ROUND(m.memory_temp::numeric, 1) as memory_temp,\n  ROUND(m.power_usage::numeric, 1) as power_usage,\n  ROUND(m.sm_active::numeric, 1) as utilization,\n  m.ecc_sbe_aggregate as sbe_errors,\n  m.ecc_dbe_aggregate as dbe_errors\nFROM (\n  SELECT DISTINCT ON (gpu_uuid) *\n  FROM gpu_metrics\n  ORDER BY gpu_uuid, time DESC\n) AS m\nLEFT JOIN gpu_assets a ON m.gpu_uuid = a.gpu_uuid\nWHERE ('$datacenter' = 'All' OR a.datacenter = '$datacenter')\n  AND ('$gpu' = 'All' OR m.gpu_uuid = '$gpu')\nORDER BY m.gpu_temp DESC",
