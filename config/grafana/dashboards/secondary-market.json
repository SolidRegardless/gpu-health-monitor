      "datasource": {
        "type": "postgres",
        "uid": "P0D1C26D7DC78F49D"
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "WITH latest_dec AS (\n  SELECT DISTINCT ON (gpu_uuid)\n    gpu_uuid, npv_keep, npv_sell, decision, health_score, rationale, confidence\n  FROM gpu_economic_decisions\n  ORDER BY gpu_uuid, time DESC\n),\nlatest_health AS (\n  SELECT DISTINCT ON (gpu_uuid) gpu_uuid, overall_score, health_grade\n  FROM gpu_health_scores ORDER BY gpu_uuid, time DESC\n),\nlatest_metrics AS (\n  SELECT DISTINCT ON (gpu_uuid) gpu_uuid, gpu_temp, sm_active\n  FROM gpu_metrics ORDER BY gpu_uuid, time DESC\n)\nSELECT\n  a.gpu_uuid AS \"UUID\",\n  h.health_grade AS \"Grade\",\n  ROUND(h.overall_score::numeric, 0) AS \"Health\",\n  EXTRACT(MONTH FROM AGE(NOW(), a.deployment_date))::int AS \"Age (mo)\",\n  ROUND(m2.gpu_temp::numeric, 1) AS \"Temp \u00b0C\",\n  ROUND((m2.sm_active)::numeric, 0) AS \"Util %\",\n  UPPER(d.decision) AS \"Decision\",\n  ROUND(d.npv_sell::numeric, 0) AS \"NPV Sell $\",\n  ROUND(d.npv_keep::numeric, 0) AS \"NPV Keep $\",\n  ROUND((d.confidence * 100)::numeric, 0) AS \"Conf %\",\n  d.rationale AS \"Analysis\"\nFROM gpu_assets a\nJOIN gpu_datacenter_mapping dm ON a.gpu_uuid = dm.gpu_uuid\nLEFT JOIN latest_dec d ON a.gpu_uuid = d.gpu_uuid\nLEFT JOIN latest_health h ON a.gpu_uuid = h.gpu_uuid\nLEFT JOIN latest_metrics m2 ON a.gpu_uuid = m2.gpu_uuid\nWHERE a.model LIKE '%A100%'\n  AND (dm.datacenter = '$datacenter' OR '$datacenter' = 'ALL')\nORDER BY h.overall_score ASC NULLS LAST",
          "format": "table"
        }
      ],
