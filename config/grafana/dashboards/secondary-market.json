    {
      "id": 11,
      "type": "barchart",
      "title": "A100 NPV vs Original Cost",
      "description": "Per-GPU comparison: original purchase cost, secondary market NPV, and operational NPV (keep value)",
      "gridPos": {
        "h": 10,
        "w": 15,
        "x": 0,
        "y": 8
      },
      "datasource": {
        "type": "postgres",
        "uid": "P0D1C26D7DC78F49D"
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "WITH latest_dec AS (\n  SELECT DISTINCT ON (gpu_uuid)\n    gpu_uuid, npv_keep, npv_sell, decision\n  FROM gpu_economic_decisions\n  ORDER BY gpu_uuid, time DESC\n),\nmkt AS (\n  SELECT DISTINCT ON (model)\n    model, price_mid_usd, original_cost_usd\n  FROM gpu_market_prices\n  WHERE condition = 'used_good'\n  ORDER BY model, fetched_at DESC\n)\nSELECT\n  a.gpu_uuid AS \"GPU\",\n  COALESCE(mp.original_cost_usd, 15000) AS \"Original Cost\",\n  ROUND(COALESCE(d.npv_sell, mp.price_mid_usd * 0.95)::numeric, 0) AS \"Secondary Market (NPV Sell)\",\n  ROUND(COALESCE(d.npv_keep, 0)::numeric, 0) AS \"Operational Value (NPV Keep)\"\nFROM gpu_assets a\nJOIN gpu_datacenter_mapping dm ON a.gpu_uuid = dm.gpu_uuid\nLEFT JOIN latest_dec d ON a.gpu_uuid = d.gpu_uuid\nLEFT JOIN mkt mp ON a.model = mp.model\nWHERE a.model LIKE '%A100%'\n  AND ('$datacenter' = 'ALL' OR dm.datacenter = '$datacenter')\nORDER BY a.gpu_uuid",
          "format": "table"
        }
      ],
      "options": {
        "xField": "GPU",
        "barWidth": 0.6,
        "groupWidth": 0.7,
        "fillOpacity": 85,
        "gradientMode": "none",
        "tooltipOptions": {
