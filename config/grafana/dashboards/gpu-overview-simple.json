{
  "title": "GPU Health Monitor - Simple",
  "uid": "gpu-health-simple",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 2,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "current": {},
        "datasource": {
          "type": "postgres",
          "uid": "P0D1C26D7DC78F49D"
        },
        "definition": "SELECT DISTINCT gpu_uuid FROM gpu_metrics ORDER BY gpu_uuid",
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "name": "gpu_uuid",
        "options": [],
        "query": "SELECT DISTINCT gpu_uuid FROM gpu_metrics ORDER BY gpu_uuid",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "sort": 0,
        "type": "query"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "GPU Temperature",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "postgres",
        "uid": "P0D1C26D7DC78F49D"
      },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "P0D1C26D7DC78F49D"
          },
          "rawSql": "SELECT\n  time AS \"time\",\n  gpu_temp AS \"Temperature\"\nFROM gpu_metrics\nWHERE gpu_uuid = '$gpu_uuid' AND $__timeFilter(time)\nORDER BY time",
          "format": "time_series",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "celsius",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "showPoints": "never"
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Power Usage",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "postgres",
        "uid": "P0D1C26D7DC78F49D"
      },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "P0D1C26D7DC78F49D"
          },
          "rawSql": "SELECT\n  time AS \"time\",\n  power_usage AS \"Power\"\nFROM gpu_metrics\nWHERE gpu_uuid = '$gpu_uuid' AND $__timeFilter(time)\nORDER BY time",
          "format": "time_series",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "showPoints": "never"
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Health Score",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 8
      },
      "datasource": {
        "type": "postgres",
        "uid": "P0D1C26D7DC78F49D"
      },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "P0D1C26D7DC78F49D"
          },
          "rawSql": "SELECT overall_score AS \"Health Score\" FROM gpu_health_scores WHERE gpu_uuid = '$gpu_uuid' ORDER BY time DESC LIMIT 1",
          "format": "table",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 60
              },
              {
                "color": "green",
                "value": 80
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "GPU Utilization",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
