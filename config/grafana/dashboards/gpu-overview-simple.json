      },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "P0D1C26D7DC78F49D"
          },
          "rawSql": "SELECT\n  time AS \"time\",\n  gpu_utilization AS \"Utilization\"\nFROM gpu_metrics\nWHERE gpu_uuid = '$gpu_uuid' AND $__timeFilter(time)\nORDER BY time",
          "format": "time_series",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100,
          "min": 0,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "showPoints": "never"
          }
        }
      }
    },
