    {
      "id": 2,
      "title": "Health Score: Current vs Predicted Trend",
      "type": "timeseries",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "postgres",
        "uid": "P0D1C26D7DC78F49D"
      },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "P0D1C26D7DC78F49D"
          },
          "rawSql": "SELECT\n  time AS \"time\",\n  overall_score AS \"Current Health Score\"\nFROM gpu_health_scores\nWHERE gpu_uuid = '$gpu_uuid' AND $__timeFilter(time)\nORDER BY time",
          "format": "time_series",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "postgres",
            "uid": "P0D1C26D7DC78F49D"
          },
          "rawSql": "WITH latest AS (\n  SELECT overall_score, time\n  FROM gpu_health_scores\n  WHERE gpu_uuid = '$gpu_uuid'\n  ORDER BY time DESC\n  LIMIT 1\n),\ntrend AS (\n  SELECT \n    REGR_SLOPE(overall_score, EXTRACT(EPOCH FROM time)) * 3600 AS hourly_change\n  FROM gpu_health_scores\n  WHERE gpu_uuid = '$gpu_uuid' AND time > NOW() - INTERVAL '6 hours'\n)\nSELECT\n  time + INTERVAL '1 hour' * s AS \"time\",\n  overall_score + (hourly_change * s) AS \"Predicted (Linear)\"\nFROM latest, trend, generate_series(0, 24) s\nWHERE time + INTERVAL '1 hour' * s <= NOW() + INTERVAL '24 hours'",
          "format": "time_series",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "max": 100,
          "min": 0,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Current Health Score"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "blue"
                }
              },
              {
                "id": "custom.lineWidth",
                "value": 3
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Predicted (Linear)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "purple"
                }
              },
              {
                "id": "custom.lineStyle",
                "value": {
                  "fill": "dash",
                  "dash": [
                    10,
                    5
                  ]
                }
              },
              {
                "id": "custom.fillOpacity",
                "value": 5
              }
            ]
          }
        ]
      }
    },
    {
      "id": 3,
      "title": "Temperature: Actual + Trend Forecast",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 9
      },
      "datasource": {
        "type": "postgres",
        "uid": "P0D1C26D7DC78F49D"
      },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "P0D1C26D7DC78F49D"
          },
          "rawSql": "SELECT\n  time AS \"time\",\n  gpu_temp AS \"Actual Temperature\"\nFROM gpu_metrics\nWHERE gpu_uuid = '$gpu_uuid' AND time > NOW() - INTERVAL '1 hour'\nORDER BY time",
          "format": "time_series",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "postgres",
            "uid": "P0D1C26D7DC78F49D"
          },
          "rawSql": "WITH latest AS (\n  SELECT gpu_temp, time\n  FROM gpu_metrics\n  WHERE gpu_uuid = '$gpu_uuid'\n  ORDER BY time DESC\n  LIMIT 1\n),\ntrend AS (\n  SELECT \n    REGR_SLOPE(gpu_temp, EXTRACT(EPOCH FROM time)) * 60 AS per_minute_change,\n    STDDEV(gpu_temp) AS volatility\n  FROM gpu_metrics\n  WHERE gpu_uuid = '$gpu_uuid' AND time > NOW() - INTERVAL '30 minutes'\n)\nSELECT\n  time + INTERVAL '1 minute' * s AS \"time\",\n  gpu_temp + (per_minute_change * s) AS \"Predicted Temp (30min)\"\nFROM latest, trend, generate_series(0, 30) s",
          "format": "time_series",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "celsius",
          "custom": {
            "lineWidth": 2
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Actual Temperature"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "blue"
                }
              },
              {
                "id": "custom.fillOpacity",
                "value": 20
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Predicted Temp (30min)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              },
              {
                "id": "custom.lineStyle",
