      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "P0D1C26D7DC78F49D"
          },
          "rawSql": "WITH latest AS (\n  SELECT thermal_health, memory_health, performance_health, power_health, reliability_health\n  FROM gpu_health_scores\n  WHERE gpu_uuid = '$gpu_uuid'\n  ORDER BY time DESC\n  LIMIT 1\n)\nSELECT 'Thermal' AS \"Component\", thermal_health AS \"Current Score\", 85.0 AS \"Baseline (Good)\"\nFROM latest\nUNION ALL\nSELECT 'Memory', memory_health, 85.0\nFROM latest\nUNION ALL\nSELECT 'Performance', performance_health, 85.0\nFROM latest\nUNION ALL\nSELECT 'Power', power_health, 85.0\nFROM latest\nUNION ALL\nSELECT 'Reliability', reliability_health, 85.0\nFROM latest",
